{
  "extends": "expo/tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "preserve",
    "moduleResolution": "bundler",

    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,

    "types": ["react", "react-native"],

    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
  "exclude": ["node_modules"]
}